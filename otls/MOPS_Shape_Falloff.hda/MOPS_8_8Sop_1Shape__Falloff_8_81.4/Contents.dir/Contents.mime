vector pp = invert(xform) * @P;
// get abs of pp and subtract box size to get bias, then get max
vector tmp = abs(pp) - radius;
if(min(radius) <= 0) {
    @mops_falloff = 0;
} else if(falloff == 0) {
    if(max(tmp) < 0) {
        @mops_falloff = 1;
