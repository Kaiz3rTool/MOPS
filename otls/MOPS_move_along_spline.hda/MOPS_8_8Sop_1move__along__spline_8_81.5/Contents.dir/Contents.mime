--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/snap_to_curve.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// move centroid to start of curve, and optionally orient all primitives as a block to the curve start.
// get mops attrs
vector center = getbbox_center(0);
vector up = chv(\"up_axis\");
vector N = chv(\"normal_axis\");
matrix3 rot = maketransform(N, up);
// get curve attrs
vector snap_P = primuv(1, \"P\", i@__curvenum, 0);
vector snap_N = primuv(1, \"mops_PT_tangent\", i@__curvenum, 0);
vector snap_up = primuv(1, \"mops_PT_normal\", i@__curvenum, 0);
matrix3 snap_rot = maketransform(snap_N, snap_up);

// move centroid to origin, then rotate to face
if(chi(\"do_orient_snap\")) {
    matrix3 snap_M = invert(rot) * snap_rot;
    @P -= center;
    @P *= snap_M;
    @P += center;
    p@orient = quaternion(snap_M);
}
// now snap to curve start
@P += (snap_P - center);
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
up_axis	[ 0	locks=0 ]	(	[ up_axisx	0 ] 	[ up_axisy	0 ] 	[ up_axisz	1 ] 	)
normal_axis	[ 0	locks=0 ]	(	[ normal_axisx	0 ] 	[ normal_axisy	1 ] 	[ normal_axisz	0 ] 	)
do_orient_snap	[ 0	locks=0 ]	(	[ do_orient_snap	1 ] 	)
}

