--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/solver1/d/s/move_along_surface.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	`chs(\"../../../../group_all\")`	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=1 ]	(	"vector dir = v@up;
float speed = ch(\"speed\");
float rand_speed = fit01(rand(@ptnum+ch(\"seed\")), -ch(\"random_speed\"), ch(\"random_speed\"));
float falloff = 1;
int do_falloff = chi(\"do_falloff\");
if(do_falloff) {
    if(haspointattrib(0, \"mops_falloff\")) {
        falloff = point(0, \"mops_falloff\", @ptnum);
    }
}

speed += rand_speed;

`ifs(ch(\"do_vexpression\"), chs(\"vexpression\"), \"\")`



// push along forward vector
vector pos = @P + (dir * speed * @mops_falloff);

// reproject onto surface

int posprim;
vector primuv;
float maxdist = ch(\"maxdist\");
xyzdist(2, pos, posprim, primuv, maxdist);
i@sourceprim = posprim;
v@sourceprimuv = primuv;
float bias = ch(\"aim_weight\");
@P = primuv(2, \"P\", posprim, primuv);
vector new_N = primuv(2, \"N\", posprim, primuv);
vector new_up = primuv(3, \"up\", posprim, primuv);
v@N = normalize(lerp(v@N, new_N, bias));
v@up = normalize(lerp(v@up, new_up, bias));"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
speed	[ 0	locks=0 ]	(	[ speed	0.01 ] 	)
maxdist	[ 0	locks=0 ]	(	[ maxdist	1 ] 	)
do_vexpression	[ 0	locks=0 ]	(	[ do_vexpression	0 ] 	)
vexpression	[ 0	locks=0 ]	(	[ vexpression	"// dir = v@up; 
// speed = 0.1;" ] 	)
aim_weight	[ 0	locks=0 ]	(	[ aim_weight	0.10000000000000001 ] 	)
seed	[ 0	locks=0 ]	(	[ seed	0 ] 	)
random_speed	[ 0	locks=0 ]	(	[ random_speed	0 ] 	)
do_falloff	[ 0	locks=0 ]	(	[ do_falloff	0 ] 	)
}

