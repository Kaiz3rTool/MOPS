Content-Disposition: attachment; filename="hdaroot/solver2/d/s/pointwrangle1.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"vector forward = chv(\"forward_vector\");
float falloff = 1;
if(chi(\"do_falloff\")) {
    if(haspointattrib(0, \"mops_falloff\")) {
        falloff = point(0, \"mops_falloff\", @ptnum);
    }
}
float speed = ch(\"speed\");
float turn_rate = ch(\"blend_in_orient\");
float rand_speed = ch(\"rand_speed\");
float seed = ch(\"seed\");

// vexpressions
`ifs(ch(\"do_vexpression\"), chs(\"vexpression\"), \"\")`

// convert forward vector to local mesh space
matrix3 m = maketransform(v@__meshN, v@__meshup);
forward = m * forward;
// project vector onto surface at current location
forward = normalize(forward - dot(forward, v@__meshN)*v@__meshN);
// advect
float rand = fit01(rand(i@id + seed), -rand_speed, rand_speed);
speed += rand;
forward = forward * speed * falloff;
@P += forward;

// get new coords
float maxdist = ch(\"project_distance\");
xyzdist(1, @P, i@__sourceprim, v@__sourceprimuv, maxdist);
@P = primuv(1, \"P\", i@__sourceprim, v@__sourceprimuv);
v@__meshN = primuv(1, \"N\", i@__sourceprim, v@__sourceprimuv);
v@__meshup = primuv(1, \"up\", i@__sourceprim, v@__sourceprimuv);

// turn towards direction of motion
vector4 end_orient = quaternion(maketransform(v@__meshN, v@__meshup));
p@orient = slerp(p@orient, qmultiply(end_orient, p@__orient_offset), turn_rate);"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
speed	[ 0	locks=0 ]	(	[ speed	0.02 ] 	)
rand_speed	[ 0	locks=0 ]	(	[ rand_speed	0 ] 	)
seed	[ 0	locks=0 ]	(	[ seed	0 ] 	)
project_distance	[ 0	locks=0 ]	(	[ project_distance	1 ] 	)
forward_vector	[ 8	locks=0 ]	(	[ forward_vectorx	0 ] 	[ forward_vectory	1 ] 	[ forward_vectorz	0 ] 	)
blend_in_orient	[ 0	locks=0 ]	(	[ blend_in_orient	0.10000000000000001 ] 	)
do_vexpression	[ 0	locks=0 ]	(	[ do_vexpression	0 ] 	)
vexpression	[ 0	locks=0 ]	(	[ vexpression	"// forward = {0,1,0};
// speed = 0.1;
// rand_speed = 0;
// seed = 0;
// falloff = @mops_falloff;
// turn_rate = 0.1;" ] 	)
do_falloff	[ 0	locks=0 ]	(	[ do_falloff	1 ] 	)
}

