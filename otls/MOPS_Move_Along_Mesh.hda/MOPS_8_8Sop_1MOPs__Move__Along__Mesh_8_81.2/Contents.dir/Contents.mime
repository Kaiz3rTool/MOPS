--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/interpolate_offsets.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// N and up here are going to be the orientation at the current frame.
// compare this orientation to the attach frame's orientation and rotate the offset vector by this.

// TRANSLATE OFFSET
vector refN = primuv(2, \"N\", i@__sourceprim, v@__sourceprimuv);
vector refup = primuv(2, \"up\", i@__sourceprim, v@__sourceprimuv);
vector currN = v@N;
vector currup = v@up;
vector4 refO = quaternion(maketransform(refN, refup));
vector4 currO = quaternion(maketransform(currN, currup));
vector4 diff = qmultiply(currO, qinvert(refO));
vector offset = v@__offsetP;
offset = qrotate(diff, offset);
v@__offset = offset;
//@P += offset;

float orient_offset = ch(\"orient_offset\");
float pos_offset = 0; // dummy definition for vexpressions
// vexpressions
`ifs(ch(\"do_vexpression\"), chs(\"vexpression\"), \"\")`

// ROTATION OFFSET
matrix3 m = primintrinsic(0, \"transform\", @ptnum);
vector4 orig_orient = quaternion(m);
p@orient = slerp(quaternion(maketransform(v@N, v@up)), orig_orient, orient_offset);
p@__orient_offset = qmultiply(qinvert(quaternion(maketransform(v@N, v@up))), orig_orient);
v@__start_N = currN;
v@__start_up = currup;"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
