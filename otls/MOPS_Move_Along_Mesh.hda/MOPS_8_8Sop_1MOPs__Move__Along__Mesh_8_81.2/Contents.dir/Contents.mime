vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// interpolate P, N, and up.
// the delta between the original N/up xform and the interpolated N/up xform
// is added to the current orientation.
vector N1 = v@__meshN;
vector up1 = v@__meshup;
vector P2 = primuv(1, \"P\", i@__sourceprim, v@__sourceprimuv);
vector N2 = primuv(1, \"N\", i@__sourceprim, v@__sourceprimuv);
vector up2 = primuv(1, \"up\", i@__sourceprim, v@__sourceprimuv);
matrix3 m1 = maketransform(N1, up1);
matrix3 m2 = maketransform(N2, up2);
matrix3 delta = invert(m1) * m2;
// apply delta to orient
p@orient = qmultiply(quaternion(delta), p@orient);

@P = P2;"	)
