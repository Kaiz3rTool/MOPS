vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
scale	[ 8	locks=0 ]	(	[ scale	1 ] 	)
amplitude	[ 0	locks=0 ]	(	[ amplitudex	0.20000000000000001 ] 	[ amplitudey	0.20000000000000001 ] 	[ amplitudez	0.20000000000000001 ] 	)
frequency	[ 0	locks=0 ]	(	[ frequencyx	1 ] 	[ frequencyy	1 ] 	[ frequencyz	1 ] 	)
offset	[ 0	locks=0 ]	(	[ offsetx	3.6099999999999999 ] 	[ offsety	0 ] 	[ offsetz	0 ] 	)
