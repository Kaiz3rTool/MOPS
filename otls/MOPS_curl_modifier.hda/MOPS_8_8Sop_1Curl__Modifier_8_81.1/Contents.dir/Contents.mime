Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	__mops_orig	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"//move packed prims

float scale = chf(\"scale\");
vector amp = chv(\"amplitude\");
vector freq = chv(\"frequency\");
vector offset = chv(\"offset\");
if(!haspointattrib(0, \"mops_falloff\")) {
    f@mops_falloff = 1;
}
float falloff = @mops_falloff;
if(!chi(\"do_falloff\")) {
    falloff = 1;
}

`ifs(ch(\"do_vexpression\"), chs(\"vexpression\"), \"\")`

vector move = curlxnoise(v@P * freq + offset);
vector origP = v@P;
v@P += move * amp * scale * falloff;
vector outN = normalize(v@P - origP);

// set orientation
matrix3 delta = dihedral(v@N, outN);
vector out_up = normalize(delta * v@up);
vector4 orig_orient = quaternion(maketransform(v@N, v@up));
vector4 out_orient = quaternion(maketransform(outN, out_up));
p@orient = slerp(orig_orient, out_orient, scale*falloff);
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
scale	[ 0	locks=0 ]	(	[ scale	1 ] 	)
amplitude	[ 0	locks=0 ]	(	[ amplitudex	0.20000000000000001 ] 	[ amplitudey	0.20000000000000001 ] 	[ amplitudez	0.20000000000000001 ] 	)
frequency	[ 0	locks=0 ]	(	[ frequencyx	1 ] 	[ frequencyy	1 ] 	[ frequencyz	1 ] 	)
offset	[ 0	locks=0 ]	(	[ offsetx	3.6099999999999999 ] 	[ offsety	0 ] 	[ offsetz	0 ] 	)
do_vexpression	[ 0	locks=0 ]	(	[ do_vexpression	0 ] 	)
vexpression	[ 0	locks=0 ]	(	`chs(\"../snippet\")`	)
do_falloff	[ 0	locks=0 ]	(	[ do_falloff	1 ] 	)
}

